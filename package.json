{
  "name": "alif-manager",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=18.0.0",
    "yarn": ">=3.2.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{js,mjs,cjs,ts,mts,jsx,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,mjs,cjs,ts,mts,jsx,tsx}\" --fix",
    "postinstall": "husky install && git config --local blame.ignoreRevsFile .git-blame-ignore-revs"
  },
  "lint-staged": {
    "*.{tsx,ts}": "yarn lint:fix"
  },
  "dependencies": {
    "@mantine/core": "^7.2.2",
    "@mantine/hooks": "^7.2.2",
    "@mantine/notifications": "^7.2.2",
    "clsx": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redaxios": "^0.5.1",
    "wouter": "^3.0.0-rc.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.41.0",
    "eslint-kit": "^10.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.31",
    "postcss-preset-mantine": "^1.11.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "packageManager": "yarn@4.0.2"
}
